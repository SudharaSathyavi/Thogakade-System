<AnchorPane prefHeight="376.0" prefWidth="332.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.POS.controller.CustomerDeleteFormController">
   <children>
      <Label layoutX="40.0" layoutY="14.0" prefHeight="17.0" prefWidth="252.0" text="Customer Delete Form">
