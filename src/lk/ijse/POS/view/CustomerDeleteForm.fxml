      </Label>
      <Separator layoutX="23.0" layoutY="44.0" prefHeight="4.0" prefWidth="287.0" />
      <TextField fx:id="txtId" layoutX="16.0" layoutY="79.0" onAction="#searchCustomerOnAction" prefHeight="43.0" prefWidth="300.0" promptText="Customer Id" />
      <TextField fx:id="txtName" layoutX="17.0" layoutY="138.0" prefHeight="43.0" prefWidth="300.0" promptText="Customer Name" />
      <TextField fx:id="txtAddress" layoutX="17.0" layoutY="196.0" prefHeight="43.0" prefWidth="300.0" promptText="Customer Address" />
      <TextField fx:id="txtSalary" layoutX="16.0" layoutY="254.0" prefHeight="43.0" prefWidth="300.0" promptText="Customer Salary" />
